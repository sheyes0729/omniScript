class Point {
    x: int = 0;
    y: int = 0;
    
    init(x, y) {
        this.x = x;
        this.y = y;
    }
    
    sum() {
        return this.x + this.y;
    }
}

declare function print(i);

function main() {
    let p = new Point(10, 20);
    print(p.sum());

    let p2 = new Point(30, 40);
    print(p2.sum());
}
